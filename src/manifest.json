{
  "manifest_version": 2,
  "name": "FuzzTabs",
  "description": "Chrome extension to organize tabs in a vertical, searchable list.",
  "version": "0.1.1",
  "icons": {
    "64": "assets/icon-128.png",
    "128": "assets/icon-256.png"
  },
  "browser_action": {
    "default_icon": "assets/icon-128.png",
    "default_title": "FuzzTabs",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.bundle.js"
    ]
  },
  "permissions": [
    "tabs",
     "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' http://localhost:8097; object-src 'self'",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "windows": "Ctrl+Shift+F",
        "chromeos": "Ctrl+Shift+F",
        "linux": "Ctrl+Shift+F",
        "mac": "Command+Shift+F"
      },
      "description": "Open FuzzTabs"
    }
  }
}